<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:UI.MVVM.ViewModel"
                    xmlns:view="clr-namespace:UI.MVVM.View">
    <DataTemplate DataType="{x:Type viewModel:ProjectsPickerViewModel}">
        <view:ProjectsPickerView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModel:FunctionalityOverviewViewModel}">
        <view:FunctionalityOverviewView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModel:TypeEffectivenessViewModel}">
        <view:TypeEffectivenessView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModel:PokemonOverviewViewModel}">
        <view:PokemonOverview/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModel:DexTypeCountViewModel}">
        <view:DexTypeCountOverview/>
    </DataTemplate>
</ResourceDictionary>